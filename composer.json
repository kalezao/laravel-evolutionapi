{
    "name": "kalezao/laravel-evolutionapi",
    "description": "Laravel package for Evolution API integration with WhatsApp messaging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kalezao",
            "email": "flipkale@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kalezao\\EvolutionApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kalezao\\EvolutionApi\\EvolutionApiServiceProvider"
            ],
            "aliases": {
                "EvolutionAPI": "Kalezao\\EvolutionApi\\Facades\\EvolutionAPI"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

